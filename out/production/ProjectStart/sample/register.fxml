<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="750.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Register">
   <children>
      <ImageView fitHeight="782.0" fitWidth="1035.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@reg.png" />
         </image>
      </ImageView>
      <ToggleButton fx:id="register" layoutX="185.0" layoutY="366.0" mnemonicParsing="false" prefHeight="117.0" prefWidth="322.0" text="REGISTER" textFill="#ed156f">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="57.0" />
         </font>
      </ToggleButton>
      <ToggleButton fx:id="login" layoutX="507.0" layoutY="366.0" mnemonicParsing="false" prefHeight="117.0" prefWidth="322.0" text="LOG IN" textAlignment="CENTER" textFill="#07941a">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="57.0" />
         </font>
      </ToggleButton>
      <Label layoutX="208.0" layoutY="200.0" text="Register and Log In to Play" textFill="#3a1ac4" underline="true">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="57.0" />
         </font>
      </Label>
      <TextField fx:id="username" disable="true" layoutX="320.0" layoutY="573.0" />
      <TextField fx:id="password" disable="true" layoutX="507.0" layoutY="573.0" />
      <Label layoutX="370.0" layoutY="549.0" text="Username">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="559.0" layoutY="549.0" text="Password">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="23.0" />
         </font>
      </Label>
      <Button fx:id="save" disable="true" layoutX="133.0" layoutY="560.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="187.0" text="SAVE" textAlignment="CENTER" textFill="#d90909">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="32.0" />
         </font>
      </Button>
      <Button fx:id="go" disable="true" layoutX="694.0" layoutY="561.0" mnemonicParsing="false" prefHeight="56.0" prefWidth="187.0" text="GO!" textAlignment="CENTER" textFill="#cf3319">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="29.0" />
         </font>
      </Button>
      <Button fx:id="quit" layoutX="404.0" layoutY="617.0" mnemonicParsing="false" prefHeight="107.0" prefWidth="206.0" text="QUIT" textAlignment="CENTER" textFill="#110a5c">
         <font>
            <Font name="Tw Cen MT Condensed Extra Bold" size="65.0" />
         </font>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
